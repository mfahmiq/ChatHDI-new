{
  "name": "chathdi",
  "version": "1.0.0",
  "description": "ChatHDI - AI Assistant for R&D Engineering with Hydrogen & Renewable Energy Database",
  "author": "HDI Team",
  "private": true,
  "main": "electron.js",
  "homepage": "./",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.8",
    "@radix-ui/react-alert-dialog": "^1.1.11",
    "@radix-ui/react-aspect-ratio": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-collapsible": "^1.1.8",
    "@radix-ui/react-context-menu": "^2.2.12",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-menubar": "^1.1.12",
    "@radix-ui/react-navigation-menu": "^1.2.10",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-toast": "^1.2.11",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@supabase/supabase-js": "^2.93.2",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.507.0",
    "mammoth": "^1.11.0",
    "next-themes": "^0.4.6",
    "pdfjs-dist": "^4.4.168",
    "pptxgenjs": "^4.0.1",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.2",
    "react-resizable-panels": "^3.0.1",
    "react-router-dom": "^7.5.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "electron:dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron:build": "electron-builder build --win --mac --linux",
    "electron:build:win": "electron-builder build --win",
    "electron:build:mac": "electron-builder build --mac",
    "electron:build:linux": "electron-builder build --linux",
    "desktop:build": "yarn build && yarn electron:build",
    "desktop:build:win": "yarn build && yarn electron:build:win"
  },
  "build": {
    "appId": "com.hdi.chathdi",
    "productName": "ChatHDI",
    "copyright": "Copyright Â© 2024 HDI Team",
    "extends": null,
    "directories": {
      "output": "release_build",
      "buildResources": "public"
    },
    "npmRebuild": false,
    "asar": true,
    "files": [
      "build/**/*",
      "electron.js",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron.js"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icons/icon.png",
      "artifactName": "ChatHDI-Setup-${version}.${ext}"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icons/icon.png",
      "artifactName": "ChatHDI-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icons/icon.png",
      "artifactName": "ChatHDI-${version}.${ext}",
      "category": "Utility"
    },
    "protocols": [
      {
        "name": "ChatHDI URL Scheme",
        "schemes": [
          "chathdi"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@eslint/js": "9.23.0",
    "autoprefixer": "^10.4.20",
    "cra-template": "1.2.0",
    "electron": "31.0.0",
    "electron-builder": "24.13.3",
    "eslint": "9.23.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "15.15.0",
    "postcss": "^8.4.49",
    "react-scripts": "5.0.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17"
  }
}
